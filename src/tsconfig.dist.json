{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "../tsconfig.json",
  "include": [
    "./"
  ],
  "exclude": [
    "./**/*.spec.ts"
  ],
  "compilerOptions": {
    "module": "CommonJS",
    "declaration": true,
    "sourceMap": false,
    "noEmit": false,
    "outDir": "../dist",
    "skipLibCheck": true
  }
}